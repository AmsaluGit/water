	<div class="card-body">
            <div class="row">
                <div class="col-md-6">
					<div class="form-group ">
						<label class="control-label no-padding-right">{{ form_label(form_sells_list.product) }}</label>
						{{ form_widget(form_sells_list.product,{'attr':{'class':'form-control'}}) }}
						<div class="help-block with-errors"></div>
					</div>
					<div class="form-group ">
						<label class="control-label no-padding-right">{{ form_label(form_sells_list.specification) }}</label>
						{{ form_widget(form_sells_list.specification,{'attr':{'class':'form-control'}}) }}
						<div class="help-block with-errors"></div>
					</div>
	
					<div class="form-group ">
						<label class="control-label no-padding-right">{{ form_label(form_sells_list.quantity) }}</label>
						{{ form_widget(form_sells_list.quantity,{'attr':{'class':'form-control'}}) }}
						<div class="help-block with-errors"></div>
					</div>
				</div>
				<div class="col-md-6">
					<div class="form-group ">
						<label class="control-label no-padding-right">{{ form_label(form_sells_list.weight) }}</label>
						{{ form_widget(form_sells_list.weight,{'attr':{'class':'form-control'}}) }}
						<div class="help-block with-errors"></div>
					</div>
	
					<div class="form-group ">
						<label class="control-label no-padding-right">{{ form_label(form_sells_list.unitPrice) }}</label>
						{{ form_widget(form_sells_list.unitPrice,{'attr':{'class':'form-control'}}) }}
						<div class="help-block with-errors"></div>
					</div>
					
					<div class="form-group ">
						<label class="control-label no-padding-right"></label>
					</div>

					<input type="hidden" value="{{ id }}" name="parentId"/>

					<div class="form-group row">

						{% if not list_button_label is defined  %}
						<div class="col-4">
							<button class="form-control btn btn-info" >{{ list_button_label|default('Save') }}</button>
						</div>
						<div class="col-4">
							<button class="form-control btn  btn-warning " style="margin-right:20px;" type="reset">Reset</button>
						</div>
						{% else %}
							<input type="hidden" name="edit_list" value="{{edit_list}}">
							<div class="col-4">
							<button class="form-control btn btn-success mr-2" >{{ list_button_label|default('Save') }}</button>
							</div>
							<div class="col-4">
							<a href="{{ path('goods_delivery_index') }}" class="form-control btn btn-primary" style = "float:right;">
								Back
							</a>
							</div>
							
						{% endif %}
					</div>
				<div>
		
	</div>

</div>

{{ form_end(form_sells_list) }}