{% extends 'base.html.twig' %}

{% block title %}UserTypes index{% endblock %}

{% block body %}
    <h1>UserTypes index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Description</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for user_type in user_types %}
            <tr>
                <td>{{ user_type.id }}</td>
                <td>{{ user_type.name }}</td>
                <td>{{ user_type.description }}</td>
                <td>
                    <a href="{{ path('user_types_show', {'id': user_type.id}) }}">show</a>
                    <a href="{{ path('user_types_edit', {'id': user_type.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('user_types_new') }}">Create new</a>
{% endblock %}
